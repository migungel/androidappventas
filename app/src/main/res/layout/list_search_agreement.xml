<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="?listPreferredItemHeight"
    android:orientation="vertical"
    android:paddingBottom="8dp"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="8dp">

    <TextView
        android:id="@+id/cl_agreement_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        tools:text="Contrato: OP50-1222" />

    <TextView
        android:id="@+id/cl_agreement_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        tools:text="Contrato: Television Analogica" />

    <TextView
        android:id="@+id/s_partners_city"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        tools:text="Guayaquil" />

    <TextView
        android:id="@+id/cl_partner_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        tools:text="Byron Sanchez" />

    <TextView
        android:id="@+id/cl_partners_street"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        tools:text="Daule - Calle 9 Octubre" />







</LinearLayout>